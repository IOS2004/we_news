{
  "cli": {
    "version": ">= 8.1.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://wenews-testing.onrender.com/api",
        "EXPO_PUBLIC_CASHFREE_ENV": "SANDBOX",
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://wenews-testing.onrender.com/api",
        "EXPO_PUBLIC_CASHFREE_ENV": "SANDBOX",
        "NODE_ENV": "preview"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://wenews-testing.onrender.com/api",
        "EXPO_PUBLIC_CASHFREE_ENV": "PROD",
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
